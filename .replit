run = "npm run dev"
entrypoint = "index.js"
hidden = [".config", "package-lock.json", ".next", ".swc"]
modules = ["nodejs-22", "postgresql-16", "python-3.11"]

[nix]
channel = "stable-24_11"

[env]
NEXT_TELEMETRY_DISABLED = "1"

[deployment]
build = ["npm", "run", "build"]
deploymentTarget = "autoscale"
run = ["npm", "run", "start", "--", "-H", "0.0.0.0"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33393
externalPort = 3001

[[ports]]
localPort = 34537
externalPort = 4200

[[ports]]
localPort = 38937
externalPort = 3000

[[ports]]
localPort = 41581
externalPort = 5000

[[ports]]
localPort = 41999
externalPort = 3002

[[ports]]
localPort = 45385
externalPort = 3003

[agent]
expertMode = true
integrations = ["resend:1.0.0", "javascript_openai_ai_integrations:1.0.0", "javascript_object_storage:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Next.js Dev Server"

[[workflows.workflow]]
name = "Next.js Dev Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[objectStorage]
defaultBucketID = "replit-objstore-54cb28b0-08bd-442a-b697-cdeead87c75d"

[userenv]

[userenv.production]
NEXT_PUBLIC_BASE_URL = "https://receiptgenerator.net"

[userenv.development]
NEXT_PUBLIC_BASE_URL = "http://localhost:5000"
